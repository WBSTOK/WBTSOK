  color: #222;
}

.page-header p {
  font-size: 1.2rem;
  color: #555;
  max-width: 600px;
  margin: 0 auto;
}

/* Product Categories */
.product-categories {
  padding: 40px 20px;
  background-color: #fff;
}

.category-tabs {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  gap: 15px;
  margin-bottom: 40px;
}

.category-tab {
  background: none;
  border: none;
  padding: 10px 20px;
  font-size: 1rem;
  cursor: pointer;
  color: #555;
  border-bottom: 2px solid transparent;
  transition: all 0.3s ease;
}

.category-tab:hover {
  color: #ff6f61;
}

.category-tab.active {
  color: #ff6f61;
  border-bottom: 2px solid #ff6f61;
  font-weight: 600;
}

/* Products Grid */
.products-grid {
  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));
  gap: 30px;
  max-width: 1200px;
  margin: 0 auto;
}

.product-card {
  background: #fff;
  border-radius: 10px;
  overflow: hidden;
  box-shadow: 0 3px 10px rgba(0,0,0,0.08);
  transition: transform 0.3s ease, box-shadow 0.3s ease;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 20px;
  text-align: center;
}

.product-card:hover {
  transform: translateY(-5px);
  box-shadow: 0 5px 15px rgba(0,0,0,0.1);
}

.product-card img {
  width: 100%;
  max-width: 220px;
  height: 220px;
  object-fit: contain;
  margin-bottom: 25px;
  transition: transform 0.3s;
}

.product-card:hover img {
  transform: scale(1.05);
}

.product-card h3 {
  font-size: 1.3rem;
  margin-bottom: 10px;
  color: #333;
}

.product-card p {
  color: #666;
  margin-bottom: 20px;
  flex-grow: 1;
}

.product-card .btn {
  margin-top: auto;
}

/* Product Price Styling */
.product-price {
  font-weight: 600;
  color: #4CAF50; /* Green color for prices as requested earlier */
  margin: 15px 0;
  padding: 10px;
  background-color: #f9f9f9;
  border-radius: 8px;
  border: 1px solid #e0e0e0;
  text-align: center;
}

/* Product Model Selection Styling */
.product-model-selection {
  margin-bottom: 15px;
}

.model-select {
  width: 100%;
  padding: 10px;
  border-radius: 8px;
  border: 1px solid #ccc;
  background-color: white;
  font-size: 0.9rem;
}

/* Product Subtotal Styling */
.product-subtotal {
  margin-top: 10px;
  font-weight: 500;
  color: #4CAF50; /* Green color for subtotals as well */
}

/* Cart Total Styling */
.cart-total {
  font-size: 1.2rem;
  font-weight: 600;
  color: #4CAF50; /* Green color for cart total */
  margin-bottom: 15px;
}

/* Selling Process */
.selling-process {
  padding: 60px 20px;
  background-color: #f9f9f9;
  text-align: center;
}

.selling-process h2 {
  font-size: 2rem;
  margin-bottom: 40px;
  color: #222;
}

.process-steps {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
  gap: 30px;
  max-width: 1200px;
  margin: 0 auto;
}

.process-step {
  background: #fff;
  padding: 30px 20px;
  border-radius: 10px;
  box-shadow: 0 3px 10px rgba(0,0,0,0.08);
  position: relative;
}

.step-number {
  width: 40px;
  height: 40px;
  background-color: #ff6f61;
  color: white;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: bold;
  font-size: 1.2rem;
  position: absolute;
  top: -20px;
  left: 50%;
  transform: translateX(-50%);
}

.process-step h3 {
  margin-top: 15px;
  margin-bottom: 10px;
  color: #333;
}

.process-step p {
  color: #666;
}


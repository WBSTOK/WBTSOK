/* Hero Section Fade-In */
.hero {
    background: url("assets/Homepage.png") no-repeat center center;
    background-size: cover;
    min-height: 90vh;
    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
    padding: 0 20px;
    opacity: 0; /* Start invisible */
    transition: opacity 1.5s ease-out; /* Smooth transition */
}

.hero.fade-in {
    opacity: 1;
}

.hero.fade-out {
    opacity: 0.2; /* Fade to 20% opacity when scrolled past */
    transition: opacity 0.8s ease-out; /* Slightly faster fade out */
}

/* Optional: Smooth Transition for Text or Elements within the Hero */
.hero-inner {
    max-width: 600px;
}

/* Fade-in effect for the hero section */
.fade-in {
    opacity: 1;
    transition: opacity 1s ease-out;
  }
  
  .hero {
    opacity: 0; /* Set initial opacity to 0 */
  }

/* Define the fade-in animation */
@keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}

/* Create a class for elements that should fade in */
.fade-in {
  animation: fadeIn 1s ease-in-out;
}

/* For elements that should fade in when they enter the viewport */
.fade-in-scroll {
  opacity: 0;
  transition: opacity 1s ease-in-out;
}

.fade-in-scroll.visible {
  opacity: 1;
}

/* Fade in/out for scrolling elements */
.fade-scroll {
  opacity: 0;
  transition: opacity 0.8s ease-in-out;
}

.fade-scroll.visible {
  opacity: 1;
}

/* Optional: You can add different transition speeds for different elements */
.fade-scroll.slow {
  transition: opacity 1.2s ease-in-out;
}

.fade-scroll.fast {
  transition: opacity 0.5s ease-in-out;
}

/* Staggered animations for cards */
.grid .card:nth-child(1) { transition-delay: 0.1s; }
.grid .card:nth-child(2) { transition-delay: 0.2s; }
